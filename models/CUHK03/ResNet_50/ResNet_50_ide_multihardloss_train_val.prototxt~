name: "Res50-base"


layer {
  name: "data"
  type: "ImageData"  
  top: "data"
  top: "label"
  include {
    phase: TRAIN
  }
  transform_param {  
    crop_size: 224
    mean_value: 104
    mean_value: 117
    mean_value: 124 
    mirror: true
  }  
  image_data_param {  
    source: "market_evaluation/dataset/train_triplet.txt" 
    root_folder: "market_evaluation/"  
    shuffle: true
    new_height: 224  
    new_width: 224  
    is_color: true  
    batch_size: 16
    pair_size: 2
  }
}


