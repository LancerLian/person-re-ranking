  name: "loss1/loss"
  type: "PairFastLoss"
  bottom: "loss1/classifier_norm"
  bottom: "label" 
  top: "loss1/loss1"
  #top: "loss1/triplet_precision"
  #top: "loss1/triplet_count"
  pair_fast_loss_param {
    margin: 1
    hard_ratio: 1
  }
}


