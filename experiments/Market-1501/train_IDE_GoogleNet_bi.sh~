#!/usr/bin/env bash

CAFFE=caffe
DATASET=market
NET=googlenet
SNAPSHOTS_DIR=output/${DATASET}_train

LOG="experiments/logs/${DATASET}_re-id_${NET}.txt.`date +'%Y-%m-%d_%H-%M-%S'`"

cd $(dirname ${BASH_SOURCE[0]})/../../

mkdir -p ${SNAPSHOTS_DIR}

GLOG_logtostderr=1 ${CAFFE}/build/tools/caffe train \
  -solver models/${DATASET}/${NET}/googlenet_solver.prototxt \
  -weights data/imagenet_models/bvlc_googlenet.caffemodel  2>&1 | tee ${LOG}
